# make sure comparisons
# never get their two
# operands in memory
# run with NReg == 3, or
# adapt it!

function $test() {
@start
	%a =w copy 0
	%b =w copy 0

@loop
	%c =w phi @start 0, @loop %f
	%d =w phi @start 0, @loop %g
	%e =w phi @start 0, @loop %h
	%f =w add %c, %d
	%g =w add %c, %e
	%h =w add %e, %d
	%x =w csle %a, %b
	jnz %x, @loop, @end

@end
	ret
}
